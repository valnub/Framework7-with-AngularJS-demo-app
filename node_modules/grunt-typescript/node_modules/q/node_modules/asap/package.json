  },
  "main": "./asap.js",
  "browser": {
    "./asap": "./browser-asap.js",
    "./raw": "./browser-raw.js",
    "./test/domain": "./test/browser-domain.js"
  },
  "files": [
    "raw.js",
    "asap.js",
    "browser-raw.js",
    "browser-asap.js"
